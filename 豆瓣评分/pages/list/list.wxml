<!--pages/list/list.wxml-->
<nav-bar bind:backTap="back" 
bind:homeTap="home"
title="{{ title }}"></nav-bar>
<view class="container">
  <view class="item"
  wx:for="{{ movies }}"
  wx:key="unique"
  wx:for-item="movie">
    <movie-item movie="{{ movie }}"></movie-item>
  </view>
</view>
