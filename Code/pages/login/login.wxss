.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-items: center;
    padding: 0 30rpx;
    height: 100%;
}

.container .logo {
    width: 144rpx;
    height: 144rpx;
    margin: 40rpx 0;
}

.container .btn-login {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 10rpx;
    box-shadow: 211rpx 21rpx 10rpx  0 rgba(0, 0, 0,1);
}

.container .wechat {
    background-color: rgb(88, 189, 58);
    color: white;  
}

.container .isHaveAcount {
    margin-top: 30rpx;
    color: rgb(88, 189, 58);
    border: 2rpx solid rgb(88, 189, 58);
}

.container .protocol {
    margin-bottom: 10rpx;
    font-size: 20rpx;
    display: flex;
    position: absolute;
    bottom: 50rpx;
    width: 100%;
    flex-direction: row;
    text-align: center;
    justify-content: center;
}

.container .protocol .privacyAgreement {
    color: rgb(88, 189, 58);
    margin-left: 10rpx;
}