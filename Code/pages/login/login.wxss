.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-items: center;
    padding: 0 30rpx;
}

.container .logo {
    width: 144rpx;
    height: 144rpx;
    margin: 40rpx 0;
}

.container .btn-login {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 10rpx;
}

.container .wechat {
    background-color: rgb(88, 189, 58);
    color: white;  
}

.container .isHaveAcount {
    margin-top: 30rpx;
    color: rgb(88, 189, 58);
    border:2rpx solid rgb(88, 189, 58);
}